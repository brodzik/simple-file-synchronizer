<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox xmlns:fx="http://javafx.com/fxml/1" prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/2.2"
      fx:controller="com.brodzik.adrian.simplefilesynchronizer.ui.dashboard.DashboardView">
    <MenuBar VBox.vgrow="NEVER">
        <Menu mnemonicParsing="false" text="File">
            <MenuItem mnemonicParsing="false" onAction="#createNew" text="New"/>
            <MenuItem mnemonicParsing="false" text="Settings" onAction="#showSettings"/>
            <MenuItem mnemonicParsing="false" text="Exit" onAction="#exit"/>
        </Menu>
        <Menu mnemonicParsing="false" text="Help">
            <MenuItem mnemonicParsing="false" text="About"/>
        </Menu>
    </MenuBar>
    <SplitPane dividerPositions="0.7115987460815048" focusTraversable="true" prefHeight="338.0" prefWidth="640.0"
               VBox.vgrow="ALWAYS">
        <items>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0">
                <children>
                    <TableView fx:id="tableView" prefHeight="354.0"
                               prefWidth="515.0"
                               AnchorPane.bottomAnchor="0.0"
                               AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                        <columns>
                            <TableColumn fx:id="idColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Id"/>
                            <TableColumn fx:id="nameColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Name"/>
                            <TableColumn fx:id="sourceColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Source"/>
                            <TableColumn fx:id="destinationColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Destination"/>
                            <TableColumn fx:id="frequencyColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Frequency"/>
                            <TableColumn fx:id="enabledColumn" editable="true" prefWidth="75.0" sortType="ASCENDING"
                                         text="Enabled"/>
                        </columns>
                    </TableView>
                </children>
            </AnchorPane>
            <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="160.0" prefWidth="100.0"/>
        </items>
    </SplitPane>
    <AnchorPane id="AnchorPane">
        <children>
            <ProgressBar prefHeight="20.0" prefWidth="640.0" progress="0.0" AnchorPane.bottomAnchor="0.0"
                         AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
        </children>
    </AnchorPane>
</VBox>
